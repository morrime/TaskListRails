<%= form_for @task do |f| %>
  <%= f.label :name %>
  <%= f.text_field :name %>

  <%= f.label :description %>
  <%= f.text_field :description %>

  <%= f.label :complete %>
  <%= f.check_box :complete %>

  <%= f.label :completion_date %>
  <%= f.date_field :completion_date %>

  <%= f.submit "Submit" %>
<% end %>
